{
  "compileOnSave": false,
  "compilerOptions": {
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es5",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2017",
      "dom"
    ],
    "baseUrl": ".",
    "paths": {
      "@nx-starter-kit/*": [
        "libs/*"
      ],
      "@default/*": ["apps/default/src/app/*"],
      "@env/*": ["apps/default/src/environments/*"]
    }
  },
  "exclude": [
    "node_modules",
    "tmp"
  ]
}
