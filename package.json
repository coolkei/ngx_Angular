    "@angular/http": "^6.0.3",
    "@angular/language-service": "^6.0.3",
    "@compodoc/compodoc": "^1.1.3",
    "@nrwl/schematics": "^6.0.2",
    "@types/d3": "^3.5.17",
    "@types/hammerjs": "^2.0.35",
    "@types/jasmine": "^2.8.7",
    "@types/jasminewd2": "^2.0.2",
