    "nvd3": "^1.8.6",
    "core-js": "^2.5.7",
    "filepond-plugin-file-encode": "^1.0.4",
    "filepond-plugin-file-validate-size": "^1.0.3",
    "filepond-plugin-file-validate-type": "^1.0.3",
