    "lite-server": "^2.3.0",
    "loaders.css": "^0.1.2",
    "ng-packagr": "^2.4.4",
    "prettier": "^1.12.1",
    "protractor": "^5.3.0",
    "standard-version": "^4.4.0",
    "ts-node": "^6.0.5",
    "tslint": "^5.10.0",
    "typescript": "2.7.2",
    "@angular-devkit/build-angular": "~0.6.1"
  }
