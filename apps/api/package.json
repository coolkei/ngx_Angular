{
  "name": "api",
  "version": "0.0.0",
  "scripts": {
    "typeorm": "ts-node -r tsconfig-paths/register --project apps/api/tsconfig.app.json node_modules/.bin/typeorm",
    "typeorm:sync": "typeorm schema:sync",
    "typeorm:seeds": "typeorm migration:run",
    "typeorm:flush": "typeorm migration:revert",
    "typeorm:create": "typeorm migration:create",
    "typeorm:preserve": "yarn typeorm:sync -- -f=ormconfig && yarn typeorm:seeds -- -f=ormconfig",
    "start": "ts-node -r tsconfig-paths/register --project apps/api/tsconfig.app.json src/main.ts",
    "start:dev": "nodemon",
    "start:debug": "nodemon --config nodemon-debug.json"
  },
  "dependencies": {
    "@godaddy/terminus": "^4.1.0",
    "@nestjs/common": "^6.1.0",
    "@nestjs/core": "^6.1.0",
    "@nestjs/cqrs": "^6.0.0",
    "@nestjs/passport": "^6.0.0",
    "@nestjs/platform-express": "^6.1.0",
    "@nestjs/platform-socket.io": "^6.1.0",
    "@nestjs/swagger": "^3.0.0",
    "@nestjs/terminus": "^6.1.0",
    "@nestjs/typeorm": "^6.0.0",
    "@nestjs/websockets": "^6.1.0",
    "@nestjsx/crud": "^3.2.0",
    "@xmlking/jwks-rsa": "^1.4.3",
    "cache-manager": "^2.9.0",
    "class-transformer": "^0.2.0",
    "class-validator": "^0.9.1",
    "cli-ux": "4.9.3",
    "cls-hooked": "^4.2.2",
    "helmet": "^3.16.0",
    "kubernetes-client": "^6.12.0",
    "nest-router": "^1.0.0",
    "nodemailer": "^6.1.0",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "pg": "^7.10.0",
    "pug": "^2.0.3",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.4.0",
    "sharp": "^0.22.0",
    "swagger-ui-express": "^4.0.2",
    "typeorm": "^0.2.16",
    "typeorm-express-query-builder": "^1.1.1",
    "web-push": "^3.3.3"
  },
  "devDependencies": {
    "@nestjs/microservices": "^6.1.0",
    "@types/helmet": "^0.0.43",
    "@types/nodemailer": "^4.6.0",
    "@types/passport": "1.0.0",
    "@types/passport-jwt": "^3.0.1",
    "@types/socket.io": "^2.1.0",
    "@types/supertest": "^2.0.0",
    "@types/web-push": "^3.3.0",
    "mongoose": "^5.5.2"
  },
  "optionalDependencies": {
    "@nestjs/schematics": "^6.1.0",
    "@nestjs/testing": "^6.1.0",
    "nodemon": "^1.18.0",
    "supertest": "^4.0.0"
  }
}
